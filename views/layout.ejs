            <div class="header-toolbar">
                <ul>
                    <% if(typeof name != 'undefined') { %> 
                        <li class="userName">Hola <%= name %></li>
                    <% } else { %>    
                        <li class="userName">Hola</li>
                    <% }  %>
                    <li><a href="/login/dashboard">
                        <i class="far fa-user"></a></i>
                    </li>
                    <li>
                        <a href="/api/products/shopping-cart"><img src="/img/shopping_cart.png" alt=""/></a>
                    </li>
                    <li><a href="">Ayuda</a></li>
                </ul>
            </div>
            </div><!--END container -->
        </nav><!-- END #header-nav -->
